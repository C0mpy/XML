<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
    xmlns:act="www.tim7.org">

    <xs:import namespace="www.tim7.org" schemaLocation="act.xsd"/>
    <xs:element name="amendment">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="act:preambula"/>
                <xs:element maxOccurs="unbounded" ref="act:clan"/>
            </xs:sequence>
            <xs:attribute name="naslov" type="xs:string" use="required"/>
        </xs:complexType>
    </xs:element>
</xs:schema>
